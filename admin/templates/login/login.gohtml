<html lang="de">
<head>
    <title>ruehmkorf.com â€“ Login</title>
    <link rel="stylesheet" type="text/css" href="https://use.typekit.net/tmw4jig.css">
    <link rel="stylesheet" type="text/css" href="/public/css/body.css">
    <link rel="stylesheet" type="text/css" href="/public/css/forms.css">
    <link rel="stylesheet" type="text/css" href="/public/css/menu.css">
    <link rel="stylesheet" type="text/css" href="/public/css/error.css">
    <link rel="stylesheet" type="text/css" href="/public/css/card.css">
    <link rel="stylesheet" type="text/css" href="/public/css/topic.css">
    <link rel="stylesheet" type="text/css" href="/public/css/news.css">
    <link rel="stylesheet" type="text/css" href="/public/css/footer.css">
    <link rel="stylesheet" type="text/css" href="/public/css/bom.css">
    <link rel="stylesheet" type="text/css" href="/public/css/download.css">
    <link rel="stylesheet" type="text/css" href="/public/css/profile.css">
    <link rel="stylesheet" type="text/css" href="/public/css/responsive.css">
</head>
<body>
<div class="rc-content">
    <form action="/admin/login" method="post" class="rc-form rc-form--login">
        {{if .Message}}
            <div class="rc-message">
                {{.Message}}
            </div>
        {{end}}
        <div class="rc-input__group">
            <label class="rc-input__label" for="email">Email</label>
            <input class="rc-input__field" type="email" value="{{.Email}}" id="email" name="email">
        </div>
        <div class="rc-input__group">
            <label class="rc-input__label" for="password">Password</label>
            <input class="rc-input__field" type="password" id="password" name="password">
        </div>
        <div class="rc-button__group">
            <button class="rc-button" type="submit">Login</button>
        </div>
    </form>
</div>
</body>
</html>